                                            <Path.Effect>
                                                <DropShadowEffect ShadowDepth="10"
                                                                  BlurRadius="1"/>
                                            </Path.Effect>
                                        </Path>
                                        <TextBlock Text="Book"
                                                   HorizontalAlignment="Center"
                                                   VerticalAlignment="Center"
                                                   FontSize="30"
                                                   Foreground="White"
                                                   FontWeight="Bold">
                                            <TextBlock.Effect>
                                                <DropShadowEffect ShadowDepth="5"/>
                                            </TextBlock.Effect>
                                        </TextBlock>
                                    </Grid>
                                </Button.Content>
                            </Button>
                        </Grid>
                        <Grid Grid.Column="1">
                            <Button Background="Transparent"
                                    Name="ButtonAddBorrower"
                                    Click="ButtonAddBorrower_Click">
                                <Button.Content>
                                    <Grid>
                                        <Path Width="200" Height="200" Stretch="Uniform" Fill="#4950DE" Data="F1 M 131.432,90.6536C 128.611,76.0937 89.7838,77.6419 80.4271,66.2396C 78.4531,63.8334 78.4531,63.8334 79.776,54.8972C 82.0338,44.8945 84.0807,51.103 87.3098,31.1524C 90.0235,14.3972 84.2357,1.52588e-005 66.6667,1.52588e-005C 49.0976,1.52588e-005 43.3105,14.3972 46.0228,31.1524C 49.252,51.103 51.2975,44.8945 53.5573,54.8972C 54.8802,63.8334 54.8802,63.8334 52.907,66.2396C 43.5495,77.6419 4.72269,76.0937 1.90106,90.6536C 1.56641,92.3763 0,101.911 0,101.911L 133.333,101.911C 133.333,101.911 131.767,92.3763 131.432,90.6536 Z ">
                                            <Path.Effect>
                                                <DropShadowEffect ShadowDepth="10"
                                                                  BlurRadius="1"/>
                                            </Path.Effect>
                                        </Path>
                                        <TextBlock Text="Borrower"
                                                   HorizontalAlignment="Center"
                                                   VerticalAlignment="Center"
                                                   FontSize="30"
                                                   Foreground="White"
                                                   FontWeight="Bold">
                                            <TextBlock.Effect>
                                                <DropShadowEffect ShadowDepth="5"/>
                                            </TextBlock.Effect>
                                        </TextBlock>
                                    </Grid>
                                </Button.Content>
                            </Button>
                        </Grid>
                        <Grid Grid.Column="2">
                            <Button Name="ButtonAddCategory"
                                    Click="ButtonAddCategory_Click"
                                    Background="Transparent">
                                <Button.Content>
                                    <TextBlock Text="Category"
                                               FontSize="30"
                                               Foreground="White"
                                               FontWeight="Bold">
                                        <TextBlock.Effect>
                                            <DropShadowEffect ShadowDepth="5"/>
                                        </TextBlock.Effect>
                                    </TextBlock>
                                </Button.Content>
                            </Button>
                        </Grid>
                    </Grid>
                </TabItem>
                <TabItem>
                    <TabItem.Header>
                        <TextBlock Text="More..."
                                   Foreground="White"
                                   Padding="40,0,40,0"
                                   FontSize="15"
                                   FontWeight="DemiBold">
                            <TextBlock.Effect>
                                <DropShadowEffect ShadowDepth="1"/>
                            </TextBlock.Effect>
                        </TextBlock>
                    </TabItem.Header>
                    <Grid Height="300" VerticalAlignment="Center">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                        </Grid.ColumnDefinitions>
                        <Button Grid.Column="0"
                                Background="Transparent"
                                Name="ButtonBooksList"
                                Click="ButtonBooksList_Click">
                            <Button.Content>
                                <Grid>
                                    <Path Width="200" Height="200" Stretch="Uniform" UseLayoutRounding="False" Fill="#4950DE" Data="F1 M 26.751,45.4141C 37.0566,45.4141 45.4189,37.0537 45.418,26.7471C 45.418,16.4414 37.0566,8.08008 26.751,8.08008C 16.4453,8.08008 8.08398,16.4414 8.08398,26.7471C 8.08301,37.0537 16.4453,45.4141 26.751,45.4141 Z M 49.4063,40.9199L 77.085,68.5977L 68.5996,77.084L 40.9219,49.4053C 36.8086,51.9834 31.9609,53.498 26.749,53.498C 11.9766,53.498 0,41.5215 0,26.75C 0,11.9766 11.9756,0 26.749,0C 41.5234,0 53.501,11.9766 53.5,26.748C 53.5,31.96 51.9844,36.8076 49.4063,40.9199 Z M 15.2686,18.8125C 18.4004,14.042 22.7471,11.3623 24.9785,12.8262C 27.21,14.291 26.4805,19.3457 23.3496,24.1182C 20.2178,28.8887 15.8701,31.5674 13.6387,30.1045C 11.4102,28.6426 12.1377,23.5801 15.2686,18.8125 Z ">
                                        <Path.Effect>
                                            <DropShadowEffect BlurRadius="1"
                                                              ShadowDepth="10"/>
                                        </Path.Effect>
                                    </Path>
                                    <TextBlock Text="Books"
                                               VerticalAlignment="Center"
                                               HorizontalAlignment="Center"
                                               FontSize="30"
                                               FontWeight="Bold"
                                               Foreground="White">
                                        <TextBlock.Effect>
                                            <DropShadowEffect ShadowDepth="1"/>
                                        </TextBlock.Effect>
                                    </TextBlock>
                                </Grid>
                            </Button.Content>
                        </Button>
