<Window x:Class="LIBRARYMANAGEMENTPART2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LIBRARYMANAGEMENTPART2"
        mc:Ignorable="d"
        Title="MainWindow" Height="Auto" Width="Auto" WindowState="Maximized" WindowStyle="None">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="35"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0" Background="#4950DE">
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
                <ColumnDefinition Width="Auto"/>
            </Grid.ColumnDefinitions>
            <Grid Grid.Column="0">
                <TextBlock Text="ED Library Management System"
                       VerticalAlignment="Center"
                       FontSize="20"
                       Margin="10,0,0,0"
                       Foreground="White"
                       FontWeight="SemiBold">
                    <TextBlock.Effect>
                        <DropShadowEffect ShadowDepth="2"/>
                    </TextBlock.Effect>
                </TextBlock>
            </Grid>
            <Grid Grid.Column="1">
                <Button Background="Transparent"
                        Name="ButtonExitProgram"
                        Click="ButtonExitProgram_Click">
                    <Button.Content>
                        <Path Width="23.3774" Height="23.46" Stretch="Uniform" Fill="White" Data="F1 M 4.70432,0L 0.0480347,4.77802L 7.00842,11.6812L 0,18.7292L 4.70432,23.46L 11.6647,16.412L 18.6252,23.46L 23.3774,18.7774L 16.369,11.6812L 23.3294,4.77802L 18.6252,0L 11.6647,6.9986L 4.70432,0 Z "/>
                    </Button.Content>
                </Button>
            </Grid>
        </Grid>
        <Grid Grid.Row="1">
            <Grid.Background>
                <ImageBrush ImageSource="bookmainbackground.jpg"
                            Opacity="1"/>
            </Grid.Background>
        </Grid>
        <Grid Grid.Row="1">
            <Grid.Background>
                <LinearGradientBrush StartPoint="-0.5,0" EndPoint="0.5,0">
                    <GradientStop Color="Transparent" Offset="0"/>
                    <GradientStop Color="#FFEAD3" Offset="1.1"/>
                </LinearGradientBrush>
            </Grid.Background>
            <TabControl Background="Transparent">
                <TabControl.Resources>
                    <Style TargetType="TabItem">
                        <Setter Property="Template">
                            <Setter.Value>
                                <ControlTemplate TargetType="TabItem">
                                    <Border Name="Border" BorderThickness="1,1,1,0" BorderBrush="#4950DE" CornerRadius="5,5,0,0">
                                        <ContentPresenter ContentSource="Header" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                    </Border>
                                    <ControlTemplate.Triggers>
                                        <Trigger Property="IsSelected" Value="True">
                                            <Setter TargetName="Border" Property="Background" Value="#4950DE"/>
                                        </Trigger>
                                        <Trigger Property="IsSelected" Value="False">
                                            <Setter TargetName="Border" Property="Background" Value="LightBlue"/>
                                        </Trigger>
                                    </ControlTemplate.Triggers>
                                </ControlTemplate>
                            </Setter.Value>
                        </Setter>
                    </Style>
                </TabControl.Resources>
                <TabItem>
                    <TabItem.Header>
                        <TextBlock Text="Borrow Books"
                                   Foreground="White"
                                   Padding="40,0,40,0"
                                   FontSize="15"
                                   FontWeight="DemiBold">
                            <TextBlock.Effect>
                                <DropShadowEffect ShadowDepth="1"/>
                            </TextBlock.Effect>
                        </TextBlock>
                    </TabItem.Header>
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="0.4*"/>
                            <ColumnDefinition/>
                        </Grid.ColumnDefinitions>
                        <Grid Grid.Column="0">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <Grid Grid.Row="0" Margin="10">
                                <StackPanel Orientation="Vertical">
                                    <TextBlock Text="Search Borrower"
                                               FontWeight="Bold"/>
                                    <TextBox Name="TextBoxSearch"
                                             Text="{Binding Path=SearchTextBorrower, UpdateSourceTrigger=PropertyChanged}"/>
                                </StackPanel>
                            </Grid>
                            <Grid Grid.Row="1" Margin="10">
                                <StackPanel Orientation="Vertical">
                                    <TextBlock Text="Sort Borrowers"
                                               FontWeight="Bold"/>
                                    <GroupBox>
                                        <GroupBox.Header>
                                            <TextBlock Text="Lastname"/>
                                        </GroupBox.Header>
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition/>
                                                <ColumnDefinition/>
                                            </Grid.ColumnDefinitions>
                                            <RadioButton Content="Ascending"
                                                         Grid.Column="0"
                                                         Name="RadioButtonLastNameAscending"
                                                         Checked="RadioButtonLastNameAscending_Checked"/>
                                            <RadioButton Content="Descending"
                                                         Grid.Column="1"
                                                         Name="RadioButtonLastNameDescending"
                                                         Checked="RadioButtonLastNameDescending_Checked"/>
                                        </Grid>
                                    </GroupBox>
                                    <GroupBox>
                                        <GroupBox.Header>
                                            <TextBlock Text="Firstname"/>
                                        </GroupBox.Header>
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition/>
                                                <ColumnDefinition/>
                                            </Grid.ColumnDefinitions>
                                            <RadioButton Content="Ascending"
                                                         Grid.Column="0"
                                                         Name="RadioButtonFirstNameAscending"
                                                         Checked="RadioButtonFirstNameAscending_Checked"/>
                                            <RadioButton Content="Descending"
                                                         Grid.Column="1"
                                                         Name="RadioButtonFirstNameDescending"
                                                         Checked="RadioButtonFirstNameDescending_Checked"/>
                                        </Grid>
                                    </GroupBox>
                                    <GroupBox>
                                        <GroupBox.Header>
                                            <TextBlock Text="ID Number"/>
                                        </GroupBox.Header>
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition/>
                                                <ColumnDefinition/>
                                            </Grid.ColumnDefinitions>
                                            <RadioButton Content="Ascending"
                                                         Grid.Column="0"
                                                         Name="RadioButtonIDNumberAscending" Checked="RadioButtonIDNumberAscending_Checked"/>
                                            <RadioButton Content="Descending"
                                                         Grid.Column="1"
                                                         Name="RadioButtonIDNumberDescending"
                                                         Checked="RadioButtonIDNumberDescending_Checked"/>
                                        </Grid>
                                    </GroupBox>
                                    <Button Background="#4950DE"
                                            Width="150"
                                            Margin="10"
                                            Name="ButtonDefaultSort"
                                            Click="ButtonDefaultSort_Click">
                                        <Button.Content>
                                            <TextBlock Text="Set sort to default"
                                                       Foreground="White"
                                                       FontWeight="Bold"/>
                                        </Button.Content>
                                    </Button>
                                </StackPanel>
                            </Grid>
                            <Grid Grid.Row="2" Margin="10,0,0,0">
                                <TextBlock Text="Borrower Options"
                                           FontWeight="Bold"/>
                            </Grid>
                            <Grid Grid.Row="3">
                                <Grid.RowDefinitions>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition/>
                                    <ColumnDefinition/>
                                </Grid.ColumnDefinitions>
                                <Button Grid.Row="0"
                                        Grid.Column="0"
                                        Margin="5"
                                        Background="#4950DE"
                                        Name="ButtonBorrowBook"
                                        Click="ButtonBorrowBook_Click">
                                    <Button.Content>
                                        <TextBlock Text="Borrow"
                                                   Foreground="White"
                                                   FontWeight="Bold"/>
                                    </Button.Content>
                                </Button>
                                <Button Grid.Row="0"
                                        Grid.Column="1"
                                        Margin="5"
                                        Background="#4950DE"
                                        Name="ButtonReturnBook"
                                        Click="ButtonReturnBook_Click">
                                    <Button.Content>
                                        <TextBlock Text="Return"
                                                   Foreground="White"
                                                   FontWeight="Bold"/>
                                    </Button.Content>
                                </Button>
                                <Button Grid.Row="1"
                                        Grid.Column="0"
                                        Margin="5"
                                        Background="#4950DE"
                                        Name="ButtonEditBorrower"
                                        Click="ButtonEditBorrower_Click">
                                    <Button.Content>
                                        <TextBlock Text="Edit"
                                                   Foreground="White"
                                                   FontWeight="Bold"/>
                                    </Button.Content>
                                </Button>
                                <Button Grid.Row="1"
                                        Grid.Column="1"
                                        Margin="5"
                                        Background="#4950DE"
                                        Name="ButtonDeleteBorrower"
                                        Click="ButtonDeleteBorrower_Click">
                                    <Button.Content>
                                        <TextBlock Text="Delete"
                                                   Foreground="White"
                                                   FontWeight="Bold"/>
                                    </Button.Content>
                                </Button>
                            </Grid>
                            <Grid Grid.Row="3" Margin="0,20,0,20">
                                
                            </Grid>
                        </Grid>
                        <Grid Grid.Column="1">
                            <ListView Background="Transparent"
                                      Name="ListViewBorrowers"
                                      ItemsSource="{Binding Path=BORROWERSLIST}"
                                      SelectedItem="{Binding Path=SelectedBorrower}">
                                <ListView.View>
                                    <GridView>
                                        <GridView.Columns>
                                            <GridViewColumn>
                                                <GridViewColumn.Header>
                                                    <TextBlock Text="Lastname"/>
                                                </GridViewColumn.Header>
                                                <GridViewColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <TextBlock Text="{Binding Path=BorrowerLastName}"/>
                                                    </DataTemplate>
                                                </GridViewColumn.CellTemplate>
                                            </GridViewColumn>
                                            <GridViewColumn>
                                                <GridViewColumn.Header>
                                                    <TextBlock Text="Firstname"/>
                                                </GridViewColumn.Header>
                                                <GridViewColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <TextBlock Text="{Binding Path=BorrowerFirstName}"/>
                                                    </DataTemplate>
                                                </GridViewColumn.CellTemplate>
                                            </GridViewColumn>
                                            <GridViewColumn>
                                                <GridViewColumn.Header>
                                                    <TextBlock Text="Gender"/>
                                                </GridViewColumn.Header>
                                                <GridViewColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <TextBlock Text="{Binding Path=BorrowerGender}"/>
                                                    </DataTemplate>
                                                </GridViewColumn.CellTemplate>
                                            </GridViewColumn>
                                            <GridViewColumn>
                                                <GridViewColumn.Header>
                                                    <TextBlock Text="Borrower Type"/>
                                                </GridViewColumn.Header>
                                                <GridViewColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <TextBlock Text="{Binding Path=BorrowerType}"/>
                                                    </DataTemplate>
                                                </GridViewColumn.CellTemplate>
                                            </GridViewColumn>
                                            <GridViewColumn>
                                                <GridViewColumn.Header>
                                                    <TextBlock Text="ID Number"/>
                                                </GridViewColumn.Header>
                                                <GridViewColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <TextBlock Text="{Binding Path=BorrowerIDNumber}"/>
                                                    </DataTemplate>
                                                </GridViewColumn.CellTemplate>
                                            </GridViewColumn>
                                            <GridViewColumn>
                                                <GridViewColumn.Header>
                                                    <TextBlock Text="Check Borrowing Book"/>
                                                </GridViewColumn.Header>
                                                <GridViewColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <TextBlock Text="{Binding Path=BorrowerCheckBorrowingBooks}"/>
                                                    </DataTemplate>
                                                </GridViewColumn.CellTemplate>
                                            </GridViewColumn>
                                        </GridView.Columns>
                                    </GridView>
                                </ListView.View>
                            </ListView>
                        </Grid>
                    </Grid>
                </TabItem>
                <TabItem>
                    <TabItem.Header>
                        <TextBlock Text="Add"
                                   Padding="50,0,50,0"
                                   Foreground="White"
                                   FontSize="15"
                                   FontWeight="DemiBold">
                            <TextBlock.Effect>
                                <DropShadowEffect ShadowDepth="1"/>
                            </TextBlock.Effect>
                        </TextBlock>
                    </TabItem.Header>
                    <Grid Height="300" VerticalAlignment="Center">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                        </Grid.ColumnDefinitions>
